    "docker:boot": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 docker-compose --env-file ./docker/.env -f docker/docker-compose.yaml -f docker/docker-compose.dev.yaml up -d",
    "docker:stop": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 docker-compose --env-file ./docker/.env -f docker/docker-compose.yaml -f docker/docker-compose.dev.yaml down",
